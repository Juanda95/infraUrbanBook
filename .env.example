# UrbanBook Infrastructure - Environment Variables
# Copy this file to .env and customize as needed

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres_password_123
POSTGRES_DB=UrbanBook
POSTGRES_INITDB_ARGS=--encoding=UTF8 --locale=es_ES.UTF-8

# API Configuration
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://+:5232
CONNECTION_STRING=Host=postgres;Port=5432;Database=UrbanBook;Username=postgres;Password=postgres_password_123;Trust Server Certificate=true;

# Docker Network
COMPOSE_PROJECT_NAME=urbanbook
